    "version": "0.2.0",
    "envFile": "${workspaceFolder}/.env",
    "configurations": [
        {
            "name": "Python: cyberbattle_gym",
            "type": "python",
            "request": "launch",
            "program": "scripts/run.py",
            "console": "integratedTerminal"
        },
        {
            "name": "Python: dql tinymicro",
            "type": "python",
            "request": "launch",
            "program": "cyberbattle/agents/baseline/notebooks/notebook_dql_debug_with_tinymicro.py",
            "console": "integratedTerminal"
        },
        {
            "name": "Python: tinymicro",
            "type": "python",
            "request": "launch",
            "program": "cyberbattle/agents/baseline/notebooks/notebook_debug_tinymicro.py",
            "console": "integratedTerminal"
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        }
    ]
}